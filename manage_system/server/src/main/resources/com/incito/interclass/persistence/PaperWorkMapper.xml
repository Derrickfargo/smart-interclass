<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.incito.interclass.persistence.PaperWorkMapper">

	<insert id="save" parameterType="com.incito.interclass.entity.cloud.PaperWork" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO
			tb_cld_quizpaper(type,filesize,path,imei,filename,teacher_id,author_name,quizid,ctime)
		VALUES
			(#{type},#{filesize},#{path},#{imei},#{filename},#{teacher_id},#{author_name},#{quizid},now())
	</insert>
	
</mapper>